<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%#resource css loader%>
    <%resources.css.forEach(function (css) { %>
        <link rel="stylesheet" href="<%- css %>"/>
    <% }); %>
    <%#assets css loader%>
    <%
    assets.css.forEach(function (css) { %>
    <link rel="stylesheet" href="<%- css %>"/>
    <% }); %>
</head>